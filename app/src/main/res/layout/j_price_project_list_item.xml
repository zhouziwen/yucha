<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/j_price_project_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/image_selector"
    >

    <TextView
        android:id="@+id/XPrice_listItem_title"
        android:text="【201622223382】西安市天然气总公司原煤气厂变压器改造项目竞价竞价竞价"
        style="@style/XPrice_listItem_titleTxt"/>

    <TextView
        android:id="@+id/XPrice_listItem_time"
        style="@style/XPrice_listItem_textBase_style"
        android:layout_below="@+id/XPrice_listItem_title"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:drawableLeft="@mipmap/price_location2"
        android:text="陕西省"
        android:gravity="center_vertical"
        />

    <TextView
        android:id="@+id/XPrice_listItem_location"
        style="@style/XPrice_listItem_textBase_style"
        android:layout_below="@+id/XPrice_listItem_title"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/XPrice_listItem_time"
        android:drawableLeft="@mipmap/price_type2"
        android:text="建筑"
        android:gravity="center_vertical"
        />

    <TextView
        android:id="@+id/XPrice_listItem_type"
        android:layout_below="@+id/XPrice_listItem_location"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="5dp"
        android:text="中标后"
        android:gravity="center_vertical"
        android:drawableLeft="@mipmap/price_stage1"
        style="@style/XPrice_listItem_textBase_style"/>

    <TextView
        android:id="@+id/XPrice_listItem_type1"
        style="@style/XPrice_listItem_textBase_style"
        android:layout_alignLeft="@+id/XPrice_listItem_location"
        android:layout_alignStart="@+id/XPrice_listItem_location"
        android:layout_alignTop="@+id/XPrice_listItem_type"
        android:drawableLeft="@mipmap/price_tishi"
        android:gravity="center_vertical"
        android:text="截止时间：2016-12-14"
        android:padding="2dp"
        />


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_below="@+id/XPrice_listItem_type"
        />













</RelativeLayout>